#!/usr/bin/env python3

import math
import sys

def factorize(n):
    factors = []
    for i in range(2, int(math.sqrt(n)) + 1):
        while n % i == 0:
            factors.append(i)
            n //= i
    if n > 1:
        factors.append(n)
    return factors

def factorize_file(input_file):
    with open(input_file, 'r') as file:
        numbers = [int(line.strip()) for line in file]

    factorizations = []
    for number in numbers:
        factors = factorize(number)
        factorizations.append((number, factors))

    return factorizations

def main():
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        sys.exit(1)

    input_file = sys.argv[1]
    factorizations = factorize_file(input_file)

    for number, factors in factorizations:
        print(f"{number} = {' * '.join(map(str, factors))}")

if __name__ == "__main__":
    main()
